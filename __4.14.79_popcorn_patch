+int pci_kmsg_send_long(unsigned int dest_cpu,
+		       struct pcn_kmsg_message *lmsg,
+		       unsigned int payload_size);
+
+#ifdef TEST_MSG_LAYER
+pcn_kmsg_cbftn callbacks[PCN_KMSG_TYPE_MAX];

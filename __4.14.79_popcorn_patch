+static atomic_t exec_count;
+static atomic_t send_count;
+
+static atomic_t timer_send_count;
+static atomic_t timer_recv_count;
+
+static volatile pool_buffer_t recv_buf[MAX_NUM_BUF];
+struct semaphore recv_buf_cnt;
+

+	struct file *fp; /* File opened by user */
+	bool internal_user;
+	wait_queue_head_t ts_wait_queue;
+};
+
+struct dte_client_mapping {
+	u32 client_index;
+	u32 lts_index;
+	u32 reg_offset;
+	u32 shift;

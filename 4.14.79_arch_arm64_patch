+
+		linux,pci-domain = <0>;
+
+		bus-range = <0x0 0xff>;
+
+		#address-cells = <3>;
+		#size-cells = <2>;
+		device_type = "pci";
+#if (ENABLE_32BIT_OUTBOUND == 1)
+		ranges = <0x83000000 0x0 0x42000000 0x0 0x42000000 0 0x2000000>,
+			 <0x43000000 0x4 0x00000000 0x4 0x00000000 0 0x80000000>;
+#else

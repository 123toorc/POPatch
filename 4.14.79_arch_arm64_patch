+ #include <asm/pgtable.h>
+ #include <asm/tlbflush.h>
+ 
++#ifdef CONFIG_POPCORN
++#include <popcorn/types.h>
++#include <popcorn/vma_server.h>
++#endif
++
+ static const char *fault_name(unsigned int esr);
+ 
+ /*
